generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum nama_satuan{
  UNIT
  PCS
  KILOGRAM
}

model tb_barang {
  id Int @id @default(autoincrement())
  kode String @db.VarChar(10)
  nama String @db.VarChar(50)
  harga Int
  satuan nama_satuan
}

